   "source": [
    "Hour, Minute = map(int,input().split())\n",
    "#map 함수 사용법 제대로 숙지하기\n",
    "if Minute < 45 :\n",
    "    if Hour == 0:\n",
    "        print( 23, Minute + 15)\n",
    "    else:\n",
    "        print(Hour - 1, Minute + 15)\n",
    "else:\n",
    "    print(Hour, Minute - 45)"
   ]
